{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/get_views",
      "methods": ["GET"],
      "dest": "api/main.py"
    },
    {
      "src": "/get_leetcode_stats",
      "methods": ["GET"],
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
